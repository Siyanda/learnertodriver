<% breadcrumb :evaluation, @quiz %>
<%= render 'evaluations/components/header' %>
<%= render 'layouts/common/errors', record: @evaluation %>
<%= render 'form', quiz: @quiz, evaluation: @evaluation %>

<%= turbo_frame_tag "question_frame" do %>
