<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">

<%= tag.meta name: 'viewport', content: 'width=device-width, initial-scale=1' %>
<%= tag.meta name: 'theme-color', content: '#353740' %>
<%= tag.meta name: 'description', content: t('description') %>
<%= tag.meta name: 'mobile-web-app-capable',content:  'yes' %>
<%= tag.meta name: "turbo-track", content: "reload" %>
<%= tag.meta name: "turbo-action", content: "advance" %>
