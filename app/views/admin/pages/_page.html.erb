<div id="<%= dom_id page %>">
  <p>
    <strong>Title:</strong>
    <%= page.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= page.content %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= page.slug %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= page.published_at %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= page.status %>
  </p>

  <p>
    <strong>User:</strong>
    <%= page.user_id %>
  </p>

  <p>
    <strong>Parent:</strong>
    <%= page.parent_id %>
  </p>

</div>
